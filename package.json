{
  "name": "gdnethome-client",
  "version": "0.0.2",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "history-server -p 8181 -a build",
    "build-css": "node-sass-chokidar src/ -o src/",
    "build": "rm -rf ../bin/build && npm run build-css && react-scripts build && mv build ../bin/",
    "buildonly": "npm run build-css && react-scripts build",
    "dev-js": "react-scripts start",
    "dev": "npm-run-all -p watch-css dev-js",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "history-server": "^1.3.1"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.11.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.27",
    "@fortawesome/free-solid-svg-icons": "^5.11.1",
    "@fortawesome/free-regular-svg-icons": "^5.11.1",
    "@fortawesome/react-fontawesome": "^0.1.9",
    "antd": "^4.0.3",
    "axios": "^0.19.2",
    "immutability-helper": "^3.0.1",
    "node-sass-chokidar": "^1.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.19.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.0",
    "react-block-ui": "^1.3.3",
    "react-dom": "^16.13.0",
    "react-fontawesome": "^1.7.1",
    "react-redux": "^7.2.0",
    "react-redux-form": "^1.16.14",
    "react-redux-toastr": "^7.6.4",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.4.0",
    "react-select": "^3.0.8",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "socket.io-client": "^2.3.0",
    "uri-template": "^1.0.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
